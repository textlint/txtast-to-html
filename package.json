{
  "name": "@textlint/txtast-to-html",
  "version": "1.0.2",
  "description": "TxtAST to HTML string.",
  "keywords": [
    "ast",
    "html",
    "textlint",
    "util"
  ],
  "homepage": "https://github.com/textlint/txtast-to-html",
  "bugs": {
    "url": "https://github.com/textlint/txtast-to-html/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/txtast-to-html.js",
  "types": "lib/txtast-to-html.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textlint/txtast-to-html.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "test:update": "SNAPSHOT_UPDATE=1 mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch"
  },
  "devDependencies": {
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.5.7",
    "cross-env": "^5.1.3",
    "husky": "^0.14.3",
    "jsdom": "^11.5.1",
    "lint-staged": "^6.0.0",
    "markdown-to-ast": "^6.0.2",
    "mocha": "^4.1.0",
    "prettier": "^1.9.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4
  },
  "dependencies": {
    "@textlint/ast-node-types": "^4.0.0",
    "escape-goat": "^1.3.0",
    "txt-ast-traverse": "^2.0.2"
  }
}
